{
    "build": {
      "dockerfile": "Dockerfile"
    },
    
    "runArgs": [
      "--gpus",
      "all",
      "--network",
      "mlflow"
    ],
    "containerEnv": {
      "AWS_ACCESS_KEY":"mlflow",
      "AWS_SECRET_KEY":"password",
      "MLFLOW_S3_ENDPOINT_URL": "http://minio:9000"
    },
    "customizations": {
      "vscode": {
      "extensions": [
        "ms-python.python",
        "ms-toolsai.jupyter",
        "MeshIntelligentTechnologiesInc.pieces-vscode",
        "GitHub.copilot-chat"
    ]
      }
    }
  }
